{
    "name": "tarampampam/laravel-roadrunner-in-docker",
    "type": "project",
    "description": "Sample Laravel application with RoadRunner in Docker Container",
    "keywords": ["laravel", "roadrunner"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "laravel/framework": "^8.44",
        "laravel/tinker": "^2.6",
        "spiral/roadrunner-laravel": "^5.0"
    },
    "require-dev": {
        "facade/ignition": "^2.9",
        "fakerphp/faker": "^1.14.1",
        "laravel/sail": "^1.7.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^5.4",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "phpunit": "@php ./vendor/bin/phpunit -d memory_limit=512M",
        "phpstan": "@php ./vendor/bin/phpstan analyze -c ./phpstan.neon --no-progress --ansi",
        "test": [
            "@phpstan",
            "@phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
